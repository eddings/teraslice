{
    "name": "Reindex",
    "lifecycle": "once",
    "workers": 1,
    "analytics": true,
    "assets": ["elasticsearch"],
    "operations": [
        {
            "_op": "elasticsearch_reader",
            "index": "example-logs-100",
            "type": "change",
            "size": 100,
            "date_field_name": "created",
            "preserve_id": true
        },
        {
            "_op": "elasticsearch_index_selector",
            "type": "change",
            "index": "example-logs-new",
            "preserve_id": true
        },
        {
            "_op": "elasticsearch_bulk",
            "size": 50
        }
    ]
}
