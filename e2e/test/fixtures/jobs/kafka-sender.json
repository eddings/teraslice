{
    "name": "Kafka Sender",
    "lifecycle": "once",
    "workers": 1,
    "analytics": true,
    "assets": ["kafka", "elasticsearch"],
    "operations": [
        {
            "_op": "elasticsearch_reader",
            "index": "example-logs-10",
            "type": "events",
            "size": 10,
            "date_field_name": "created",
            "preserve_id": true
        },
        {
            "_op": "teraslice_kafka_sender",
            "connection": "default",
            "topic": "example-logs-10",
            "timestamp_field": "created",
            "_encoding": "json"
        }
    ]
}
