{
    "name": "Kafka Sender",
    "lifecycle": "once",
    "workers": 1,
    "analytics": true,
    "assets": ["kafka", "elasticsearch"],
    "operations": [
        {
            "_op": "elasticsearch_reader",
            "index": "example-logs-100",
            "type": "change",
            "size": 100,
            "date_field_name": "created",
            "preserve_id": true
        },
        {
            "_op": "teraslice_kafka_sender",
            "topic": "example-logs-100",
            "timestamp_field": "created"
        }
    ]
}
