{
    "name": "teraslice-cli",
    "version": "0.7.2",
    "description": "Command line manager for teraslice jobs, assets, and cluster references.",
    "keywords": [
        "teraslice"
    ],
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/teraslice-cli#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": "git@github.com:terascope/teraslice.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "files": [
        "bin/**/*",
        "dist/src/**/*"
    ],
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "bin": {
        "earl": "bin/teraslice-cli.js",
        "teraslice-cli": "bin/teraslice-cli.js"
    },
    "directories": {
        "lib": "dist/src",
        "test": "test"
    },
    "scripts": {
        "build": "tsc --build --pretty",
        "build:watch": "yarn build --watch",
        "lint": "tslint -p tsconfig.json -t verbose -e '**/*.json'",
        "lint:fix": "yarn lint --fix",
        "prepublishOnly": "yarn build",
        "test": "ts-scripts test . --",
        "test:debug": "ts-scripts test --debug . --",
        "test:watch": "ts-scripts test --watch . --"
    },
    "dependencies": {
        "@terascope/fetch-github-release": "^0.6.0",
        "@terascope/utils": "^0.14.1",
        "archiver": "^3.0.0",
        "bluebird": "^3.5.5",
        "chalk": "^2.4.2",
        "cli-table3": "^0.5.1",
        "easy-table": "^1.1.1",
        "fs-extra": "^8.1.0",
        "js-yaml": "^3.13.1",
        "lodash": "^4.17.11",
        "node-yaml": "^4.0.0",
        "prompts": "^2.1.0",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "signale": "^1.4.0",
        "teraslice-client-js": "^0.7.2",
        "tmp": "^0.1.0",
        "tty-table": "^2.7.4",
        "yargs": "^13.2.4",
        "yeoman-environment": "^2.3.4",
        "yeoman-generator": "^4.0.1",
        "yo": "^3.1.0"
    },
    "devDependencies": {
        "@types/signale": "^1.2.1",
        "@types/yargs": "^13.0.0",
        "jest-fixtures": "^0.6.0",
        "yeoman-assert": "^3.1.1",
        "yeoman-test": "^2.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "displayName": "Teraslice CLI",
    "pkg": {
        "scripts": "cmds/**/*.js",
        "assets": [
            "generators/**/templates/*"
        ]
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "testSuite": "unit",
        "enableTypedoc": true
    }
}
