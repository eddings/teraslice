{
    "name": "teraslice-cli",
    "version": "0.1.0",
    "description": "Teraslice command line interface",
    "bin": {
        "teraslice-cli": "./index.js",
        "tjm": "./index.js"
    },
    "main": "index.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "lint": "eslint cmds/**/*.js test/**/*.js *.js",
        "lint:fix": "eslint cmds/**/*.js test/**/*.js *.js --fix",
        "test": "jest",
        "test:debug": "env DEBUG='*teraslice*' jest --detectOpenHandles --coverage=false --runInBand"
    },
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/teraslice-cli#readme",
    "repository": "git@github.com:terascope/teraslice.git",
    "author": "Terascope, LLC <info@terascope.io>",
    "license": "MIT",
    "keywords": [
        "teraslice"
    ],
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "dependencies": {
        "archiver": "^3.0.0",
        "bluebird": "^3.5.2",
        "chalk": "^2.3.2",
        "fs-extra": "^7.0.0",
        "lodash": "^4.17.11",
        "prompts": "^1.1.0",
        "teraslice-client-js": "^0.1.3",
        "yargs": "^12.0.2"
    },
    "devDependencies": {
        "eslint": "^5.6.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.14.0",
        "jest": "^23.6.0",
        "jest-extended": "^0.9.0",
        "jest-fixtures": "^0.6.0"
    }
}
