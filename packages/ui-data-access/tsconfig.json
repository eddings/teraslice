{
    "compilerOptions": {
        "outDir": "./dist",
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "node",
        "sourceMap": true,
        "declaration": false,
        "resolveJsonModule": true,
        "noUnusedLocals": true,
        "jsx": "react"
    },
    "include": ["src"]
}
