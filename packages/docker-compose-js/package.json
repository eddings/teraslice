{
  "name": "@terascope/docker-compose-js",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Node.js driver for controlling docker-compose testing environments.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js spec",
    "lint:fix": "eslint --fix *.js spec",
    "test": "nyc -x spec/ --reporter=text-summary jasmine; nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov > coverage/coverage.lcov"
  },
  "nyc": {
    "cache": true,
    "all": true,
    "include": [
      "*.js"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov",
      "json"
    ]
  },
  "keywords": [
    "docker",
    "docker-compose",
    "testing"
  ],
  "homepage": "https://github.com/terascope/teraslice/tree/master/packages/docker-compose-js#readme",
  "repository": "git@github.com:terascope/teraslice.git",
  "author": "Terascope, LLC <info@terascope.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/terascope/teraslice/issues"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nyc": "^12.0.2"
  }
}
