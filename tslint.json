{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "indent": [ true, "spaces" ],
    "ter-indent": [ true, 4, { "SwitchCase": 1 }],
    "no-unused-variable": false,
    "import-name": false,
    "max-line-length": [ true, 200 ],
    "no-debugger": true,
    "trailing-comma": false,
    "comma-dangle": false
  }
}
